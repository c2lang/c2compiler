// @warnings{no-unused}
module test;

static_assert( 10 %  20,  10);
static_assert(-10 %  20, -10);
static_assert( 10 % -20,  10);
static_assert(-10 % -20, -10);
static_assert(10.0 % 2,  0);    // @error{invalid operand for binary operation remainder}
static_assert(10 % 2.0,  0);    // @error{invalid operand for binary operation remainder}
