// @warnings{no-unused}
module test;

type Inner struct {
    i32 a;
}

fn i32 Inner.get(Inner* i) {
    return i.a;
}

type Outer struct {
    Inner i;
}

fn void test1(const Outer* o) {
    Inner* i = &o.i;    // @error{pointer conversion discards const qualifier}
}

fn void test2(const Outer* o) {
    const Inner* i = &o.i;
    i32 b = i.get();    // @error{pointer conversion discards const qualifier}
}

fn void test3(const Outer* o) {
    i32 a = o.i.get();      // @error{pointer conversion discards const qualifier}
}

fn void test1v(volatile Outer* o) {
    Inner* i = &o.i;    // @error{pointer conversion discards volatile qualifier}
}

fn void test2v(volatile Outer* o) {
    volatile Inner* i = &o.i;
    i32 b = i.get();    // @error{pointer conversion discards volatile qualifier}
}

fn void test3v(volatile Outer* o) {
    i32 a = o.i.get();  // @error{pointer conversion discards volatile qualifier}
}

fn void test1cv(const volatile Outer* o) {
    Inner* i = &o.i;    // @error{pointer conversion discards qualifiers}
}

fn void test2cv(const volatile Outer* o) {
    const volatile Inner* i = &o.i;
    i32 b = i.get();    // @error{pointer conversion discards qualifiers}
}

fn void test3cv(const volatile Outer* o) {
    i32 a = o.i.get();  // @error{pointer conversion discards qualifiers}
}

