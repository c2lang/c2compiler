// @warnings{no-unused}
module test;

/*
    char
    i8
    i16
    i32
    i64
    u8
    u16
    u32
    u64
    isize
    usize
*/

func void test_char(char a) {
    char c = a;
    i8  m = a;
    i16 s = a;
    i32 i = a;
    i64 l = a;
    u8  m2 = a; // @error{implicit conversion changes signedness: 'char' to 'u8'}
    u16 s2 = a; // @error{implicit conversion changes signedness: 'char' to 'u16'}
    u32 i2 = a; // @error{implicit conversion changes signedness: 'char' to 'u32'}
    u64 l2 = a; // @error{implicit conversion changes signedness: 'char' to 'u64'}
    isize is = a;
    usize us = a; // @error{implicit conversion changes signedness: 'char' to 'usize'}
}

